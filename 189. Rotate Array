class Solution {
public:
    // void reverse(vector<int> &a, int start, int end){
    //     while(start<end){
    //         swap(a[start],a[end]);
    //         start++;
    //         end--;
    //     }
    //     return;
    // }
    void rotate(vector<int>& nums, int k) {
        int n=nums.size();
        if(n==k) return;
        if(n<k){
            k=k%n;
        }
        
        reverse(nums.begin(),nums.begin()+n-k);
        reverse(nums.begin()+n-k,nums.end());
        reverse(nums.begin(),nums.end());
        return;
    }
};
